{
  "schemaVersion": 1,
  "id": "extrahud",
  "version": "${version}",
  "name": "ExtraHUD",
  "description": "This mod shows some useful information on the screen.",
  "authors": [
    "Ceziy"
  ],
  "contact": {
    "sources": "https://github.com/Ceziy01/ExtraHUD"
  },
  "license": "MIT",
  "icon": "assets/extrahud/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.ceziy.extrahud.extrahud.ExtraHUDClient"
    ],
    "main": [
      "me.ceziy.extrahud.extrahud.ExtraHUD"
    ],
    "modmenu": [
      "me.ceziy.extrahud.extrahud.modmenu.ExtraHUDMenuApiImpl"
    ]
  },
  "mixins": [
    "extrahud.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.modrinth": "https://modrinth.com/mod/extrahud"
      }
    }
  }
}
